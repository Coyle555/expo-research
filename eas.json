{
  "cli": {
    "version": ">= 5.4.0"
  },
  "build": {
    "development": {
      "env": {
        "EXPO_PUBLIC_APP_NAME": "DEVELOPMENT (CI/CD)"
      },
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "staging": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_APP_NAME": "STAGING (CI/CD)"
      }
    },
    "production": {
      "node": "16.18.0",
      "env": {
        "EXPO_PUBLIC_APP_NAME": "PRODUCTION (CI/CD)"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
